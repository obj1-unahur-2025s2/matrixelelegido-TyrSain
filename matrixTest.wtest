import matrix.*


describe "nave de la matrix" {

  test "se suben todos a la nave y la nave acelera" {
    nave.subirPasajero([neo, morfeo, trinity])
    nave.naveAcelera()

    assert.equals(10, neo.vitalidad())
    assert.equals(7, morfeo.vitalidad())
    assert.equals(0, trinity.vitalidad())
  }

}

